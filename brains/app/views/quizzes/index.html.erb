<h1 class="text-center">Quizzes</h1>

<ol>
<% @quizzes.each do |quiz| %>
	<li>
		<%= quiz.Name %> <%= link_to "Editar", edit_quiz_path(quiz) %>
		<%= link_to "Delete", quiz_path(quiz), method: :delete, 
		data: { confirm: 'Estas seguro?'} %>
		<%= link_to "Show", quiz_path(quiz) %>
		<br>
	</li>
<% end %>
<br>


<%= link_to( "Anadir quiz",new_quiz_path, {class: 'btn btn-primary'}) %>



